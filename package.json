{
  "name": "rekindle-web",
  "version": "0.1.0",
  "private": true,
  "volta": {
    "node": "20.20.0",
    "npm": "10.9.0"
  },
  "scripts": {
    "predev": "node scripts/validate-env.mjs --runtime studio && node scripts/sync-well-known.mjs",
    "prebuild": "node scripts/validate-env.mjs --runtime studio && node scripts/sync-well-known.mjs",
    "prestart": "node scripts/validate-env.mjs --runtime studio",
    "validate:env": "node scripts/validate-env.mjs --runtime studio",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "pipeline:build": "tsc -p ./pipeline/tsconfig.json",
    "pipeline:test:rak": "npm run pipeline:build --silent && node --test ./pipeline/dist/sources/rak/parser.test.js",
    "pipeline:test:quality": "npm run pipeline:build --silent && node --test ./pipeline/dist/core/quality.test.js",
    "pipeline:list-sources": "npm run pipeline:build --silent && node ./pipeline/dist/index.js list-sources",
    "pipeline:run-source": "node scripts/validate-env.mjs --runtime pipeline && npm run pipeline:build --silent && node ./pipeline/dist/index.js run-source",
    "supabase:start": "supabase start --workdir ./db",
    "supabase:stop": "supabase stop --workdir ./db",
    "supabase:reset": "supabase db reset --workdir ./db",
    "ingest:supabase:start": "supabase start --workdir ./ingestion",
    "ingest:supabase:stop": "supabase stop --workdir ./ingestion",
    "ingest:supabase:reset": "node scripts/ingest-db-ops.mjs reset-local",
    "ingest:supabase:reset:linked": "node scripts/ingest-db-ops.mjs reset-linked",
    "ingest:cleanup:sql": "node scripts/ingest-db-ops.mjs cleanup-sql",
    "types:gen:local": "supabase gen types typescript --local --schema public > lib/database/types.gen.ts --workdir ./db"
  },
  "dependencies": {
    "@supabase/ssr": "^0.8.0",
    "@supabase/supabase-js": "^2.97.0",
    "next": "16.1.6",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "16.1.6",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
