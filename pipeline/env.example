# Durable ingestion Supabase project (separate from app DB).
INGEST_SUPABASE_URL=
INGEST_SUPABASE_SERVICE_ROLE_KEY=

# App Supabase project (for cross-DB promotion reconciliation).
APP_SUPABASE_URL=
APP_SUPABASE_SERVICE_ROLE_KEY=

# Durable snapshot settings.
INGEST_SNAPSHOT_MODE=local
INGEST_SNAPSHOT_LOCAL_DIR=./snapshots
INGEST_SNAPSHOT_BUCKET=rekindle-ingestion-snapshots

# Optional runtime settings.
INGEST_LOG_LEVEL=info
INGEST_DEFAULT_LOCALE=en
INGEST_RECONCILIATION_SPIKE_THRESHOLD=25
INGEST_RECONCILIATION_PAGE_SIZE=500

# Do not use:
# - INGEST_SUPABASE_KEY
# - APP_SUPABASE_KEY
# - NEXT_PUBLIC_SUPABASE_SERVICE_ROLE_KEY
